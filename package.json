{
  "name": "blueprint-studios",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build:admin": "tsc -p tsconfig.admin.json",
    "build:node": "tsc -p tsconfig.node.json",
    "build:app": "tsc && vite build",
    "build": "npm run build:node && npm run build:admin && npm run build:app",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "admin:setup": "ts-node -P tsconfig.admin.json src/scripts/setupAdmin.ts"
  },
  "dependencies": {
    "@types/react-helmet": "^6.1.11",
    "dotenv": "^16.3.1",
    "firebase": "^10.5.2",
    "firebase-admin": "^11.11.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.7.1",
    "lucide-react": "^0.290.0",
    "react": "^18.2.0",
    "react-calendar": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.17.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.15",
    "@types/react-calendar": "^3.9.0",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.31",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
